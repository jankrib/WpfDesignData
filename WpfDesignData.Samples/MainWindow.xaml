<Window x:Class="WpfDesignData.Samples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfDesignData.Samples"
        xmlns:vm="clr-namespace:WpfDesignData.Samples.ViewModels"
        xmlns:view="clr-namespace:WpfDesignData.Samples.Views"
        xmlns:dd="clr-namespace:WpfDesignData;assembly=WpfDesignData"
        mc:Ignorable="d"
        Height="450" Width="800">
    <Window.DataContext>
        <dd:DesignClass2>
            <dd:DesignProperty Key="Test" Value="Hei"/>
        </dd:DesignClass2>
    </Window.DataContext>
    <Grid Background="#F7FAFC">
        <TextBlock Text="{Binding Test}" />
    </Grid>
</Window>
